{
  "select": ["day", "country", "type", {"COUNT": "*"}],
  "from": "events",
  "where": [
    {"col": "day", "op": "between", "val": ["2024-01-01", "2024-01-31"]},
    {"col": "country", "op": "eq", "val": "CA"}
  ],
  "group_by": ["day", "country", "type"],
  "order_by": [
    {"col": "day", "dir": "asc"},
    {"col": "type", "dir": "asc"}
  ]
}
